import <std_connector.camkes>;

component Foo {
  control;
  consumes MyEvent incoming;
  emits MyEvent outgoing;
}

assembly {
  composition {
    component Foo foo;

    connection seL4Asynch s(from foo.outgoing, to foo.incoming);
  }
}

